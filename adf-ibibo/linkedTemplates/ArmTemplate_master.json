{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ibibo"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"linkedservice1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice1'"
		},
		"ls_AzureSqlDatabase2_param_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase2_param'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsibibo.dfs.core.windows.net/"
		},
		"param_triggers_properties_pipeline2_copy4_parameters_dbname": {
			"type": "string",
			"defaultValue": "db-ibibo"
		},
		"param_triggers_properties_pipeline2_copy4_parameters_schemaname": {
			"type": "string",
			"defaultValue": "dbo"
		},
		"param_triggers_properties_pipeline2_copy4_parameters_tablename": {
			"type": "string",
			"defaultValue": "result"
		},
		"trigger1_properties_pipeline1_parameters_Schema": {
