			"defaultValue": "dbo"
		},
		"trigger1_properties_pipeline1_parameters_Table": {
			"type": "string",
			"defaultValue": "First"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ibibo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"linkedservice1_connectionString": {
						"value": "[parameters('linkedservice1_connectionString')]"
					},
					"ls_AzureSqlDatabase2_param_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase2_param_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"param_triggers_properties_pipeline2_copy4_parameters_dbname": {
						"value": "[parameters('param_triggers_properties_pipeline2_copy4_parameters_dbname')]"
					},
					"param_triggers_properties_pipeline2_copy4_parameters_schemaname": {
						"value": "[parameters('param_triggers_properties_pipeline2_copy4_parameters_schemaname')]"
					},
					"param_triggers_properties_pipeline2_copy4_parameters_tablename": {
						"value": "[parameters('param_triggers_properties_pipeline2_copy4_parameters_tablename')]"
					},
					"trigger1_properties_pipeline1_parameters_Schema": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_Schema')]"
					},
					"trigger1_properties_pipeline1_parameters_Table": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_Table')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}